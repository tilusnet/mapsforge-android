package org.mapsforge.routing.preprocessing.hh.hierarchycomputation.util.arrays;

/**
 * synchroniyed wrapper class
 */
public class BitArraySynchronized extends BitArray {

	private static final long serialVersionUID = 6283194127482992676L;

	/**
	 * @param size
	 *            of this array.
	 */
	public BitArraySynchronized(int size) {
		super(size);
	}

	@Override
	public synchronized void clear(int i) {
		super.clear(i);
	}

	@Override
	public synchronized boolean get(int i) {
		return super.get(i);
	}

	@Override
	public synchronized void set(int i) {
		super.set(i);
	}

	@Override
	public synchronized int size() {
		return super.size();
	}
}
